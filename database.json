  "encounterTypes": [
    {
      "id": 1,
      "name": "Combat",
      "description": "Direct conflict between the player characters and one or more adversaries. It typically includes elements such as initiative rolls, tactical positioning, and resource management (like hit points and spells) as the players strive to overcome their opponents through combat prowess and strategy.",
      "image": "https://www.dicemonkey.net/wp-content/uploads/2020/02/WAg3SIj.jpg"
    },
    {
      "id": 2,
      "name": "Stealth",
      "description": "Stealth encounters emphasize the characters' ability to move unseen and unheard, avoiding detection by enemies or other obstacles. Players may need to make stealth checks, find hiding spots, or distract guards to accomplish their objectives without alerting their foes.",
      "image": "https://www.belloflostsouls.net/wp-content/uploads/2021/08/dnd-stealth-header-image.jpg"
    },
    {
      "id": 3,
      "name": "Social",
      "description": "Social encounters focus on interaction and negotiation between the player characters and non-player characters (NPCs). This could involve anything from convincing a noble to grant them an audience, persuading a guard to let them pass, or gathering information from a hesitant informant. Charisma-based skills are often crucial in these encounters.",
      "image": "https://i0.wp.com/creativegamemechanics.com/wp-content/uploads/2023/03/social-encounters-featured.jpg?fit=850%2C537&ssl=1"
    },
    {
      "id": 4,
      "name": "Puzzle",
      "description": "Puzzle encounters challenge players with intellectual and problem-solving tasks. These could range from deciphering ancient runes to unlocking a mechanism that blocks their progress. Players must use logic, deduction, and sometimes teamwork to overcome these challenges.",
      "image": "https://static.wixstatic.com/media/3bd763_2441b3175bcd4bbaa3c5db5a47c7e35c~mv2.jpg/v1/fill/w_640,h_444,al_c,q_80,usm_0.66_1.00_0.01,enc_auto/3bd763_2441b3175bcd4bbaa3c5db5a47c7e35c~mv2.jpg"
    },
    {
      "id": 5,
      "name": "Exploration",
      "description": "Exploration encounters involve the characters traversing unfamiliar terrain or delving into unknown locations. Players might uncover hidden treasures, encounter natural hazards, or stumble upon forgotten ruins. These encounters often reward curiosity and resourcefulness.",
      "image": "https://aravar27.wordpress.com/wp-content/uploads/2021/06/borderland_explorer_by_lucasgraciano_dah5dit-pre.jpg"
    },
    {
      "id": 6,
      "name": "Skill Challenge",
      "description": "Skill challenges present players with a series of tasks or obstacles that require the effective use of their character's skills and abilities. Success often depends on a combination of creativity, strategy, and clever application of their character's strengths.",
      "image": "https://static1.cbrimages.com/wordpress/wp-content/uploads/2021/04/Candlekeep-Mysteries-Key-Art-by-Clint-Cearley.jpg"
    },
    {
      "id": 7,
      "name": "Mystery",
      "description": "Mystery encounters involve unraveling enigmatic plots, solving intricate puzzles, or uncovering hidden truths. Players must piece together clues, interrogate suspects, and follow leads to uncover the secrets lurking beneath the surface. These encounters can be both intellectually stimulating and emotionally engaging.",
      "image": "https://www.dndbeyond.com/attachments/11/155/murder-mystery.jpg"
    }
  ]
